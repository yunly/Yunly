
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Razor</title>
    <link asp-href-include="lib/bootstrap/css/*.min.css" rel="stylesheet" />

    <script asp-src-include="lib/jquery/*.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            var list = $("#list");
            //JSON.parse($("#jsonData").text()).forEach(function (val) {console.log("Val: " + val); list.append($("<li>").text(val));});
            //JSON.parse($("#jsonData").text()).forEach(v => list.append($("<li>").text(v)));

            $.parseJSON($("#jsonData").text()).forEach(v => list.append("<li>" + v + "</li>"));
        });
    </script>


    <script id="jsonData" type="application/json">
        @Json.Serialize(new string[] { "Apple", "Orange", "Pear" })
    </script>
</head>
<body class="m-1 p-1">
    This is the List View
    @*<partial name="MyPartial" />*@
    @*<partial name="MyStronglyTypedPartial" model=@(new string[] { "Apple", "Orange", "Pear" }) />*@
    <ul id="list"></ul>
</body>
</html>